<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context=".Fragments.ContactanosFragment">


    <ImageView
        android:id="@+id/imageView_Tec"
        android:layout_width="300dp"
        android:layout_height="@dimen/height_imgView_215"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/uni" />

    <TextView
        android:id="@+id/txtVw_Contactos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:text="@string/contactos"
        android:textSize="@dimen/subtitles_contact_20"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/imageView_Tec"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/txt_Leti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="34dp"
        android:text="@string/name_Leti"
        android:textSize="@dimen/font_Size_txtContacto_17"
        app:layout_constraintStart_toEndOf="@+id/txtVw_Contactos"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_Rebeca"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="34dp"
        android:text="@string/name_rebe"
        android:textSize="@dimen/font_Size_txtContacto_17"
        app:layout_constraintStart_toEndOf="@+id/txt_Leti"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_telefonos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="@string/telefonos"
        android:textSize="@dimen/subtitles_contact_20"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/imageView_Tec"
        app:layout_constraintTop_toBottomOf="@+id/txtVw_Contactos" />


    <TextView
        android:id="@+id/txt_cel_Leti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="14dp"
        android:shadowColor="@color/colorPrimary"
        android:shadowDx="7"
        android:shadowDy="7"
        android:shadowRadius="15"
        android:text="@string/num_leti"
        android:textSize="@dimen/font_Size_txtContacto_17"
        app:layout_constraintStart_toEndOf="@+id/txt_telefonos"
        app:layout_constraintTop_toBottomOf="@+id/txt_Leti" />


    <TextView
        android:id="@+id/txt_cel_Rebeca"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="14dp"
        android:shadowColor="@color/colorPrimary"
        android:shadowDx="7"
        android:shadowDy="7"
        android:shadowRadius="15"
        android:text="@string/num_rebe"
        android:textSize="@dimen/font_Size_txtContacto_17"
        app:layout_constraintStart_toEndOf="@+id/txt_cel_Leti"
        app:layout_constraintTop_toBottomOf="@+id/txt_Rebeca" />

    <TextView
        android:id="@+id/txt_Correo_Electro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="@string/correosElectr"
        android:textSize="@dimen/subtitles_contact_20"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/imageView_Tec"
        app:layout_constraintTop_toBottomOf="@+id/txt_telefonos" />

    <TextView
        android:id="@+id/txt_correo_Leti"
        android:layout_width="wrap_content"
        android:layout_height="22dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:text="@string/correoLeti"
        android:textSize="@dimen/font_Size_txtContacto_17"
        app:layout_constraintStart_toEndOf="@+id/imageView_Tec"
        app:layout_constraintTop_toBottomOf="@+id/txt_Correo_Electro" />

    <TextView
        android:id="@+id/txt_correo_Rebeca"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="@string/correoRebe"
        android:textSize="@dimen/font_Size_txtContacto_17"
        app:layout_constraintStart_toEndOf="@+id/imageView_Tec"
        app:layout_constraintTop_toBottomOf="@+id/txt_correo_Leti" />


    <ImageView
        android:id="@+id/imageView_LogoTecJob"
        android:layout_width="170sp"
        android:layout_height="90sp"
        android:layout_marginTop="35dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.611"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_correo_Rebeca"
        app:srcCompat="@drawable/logot" />

    <ImageView
        android:id="@+id/imageView_Logo"
        android:layout_width="70sp"
        android:layout_height="50sp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="84dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.752"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txt_correo_Rebeca"
        app:srcCompat="@drawable/logotecgold" />


</androidx.constraintlayout.widget.ConstraintLayout>